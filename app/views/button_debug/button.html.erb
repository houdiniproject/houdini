<script>
    (function() {
        if(document.querySelector('.commitchange-donate[data-fixed]')) return;
        var cc_donate = document.createElement('div');
        cc_donate.innerHTML = "<a data-fixed class='commitchange-donate' data-custom-fields='shirt sizer: T-shirt size, company: Company name'></a>";
        document.body.appendChild(cc_donate);
        var npo = 1;
        var script = document.createElement('script');
        var first = document.getElementsByTagName('script')[0];
        script.setAttribute('data-npo-id', npo);
        script.id = 'commitchange-script';
        script.src = '/js/donate-button.v2.js';
        first.parentNode.insertBefore(script, first);
    })();
</script>
