<div class="shareButtons">
  <% if @nonprofit.facebook.present? %>
    <%= facebook_share(request.original_url) %>
  <% end %>

  <% if @nonprofit.twitter.present? %>
    <%= twitter_share(@nonprofit.name) %>
  <% end %>
</div>
